{
  "tasks": {
    "start": "deno run --env-file=.env --allow-env --allow-net --allow-read --allow-write --unstable-kv src/main.ts",
    "dev": "deno run --watch --env-file=.env --allow-env --allow-net --allow-read --allow-write --unstable-kv src/main.ts",
    "update-depth": "deno run --watch --env-file=.env --allow-env --allow-net --allow-read --allow-write --unstable-kv src/update-depth.ts",
    "kv": "deno run -A --unstable-kv kview/main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@t3-oss/env-core": "npm:@t3-oss/env-core@^0.13.4",
    "zod": "npm:zod@^3.25.28"
  }
}